{
  "name": "rsis",
  "version": "0.0.2",
  "description": "rsis (R Stock-In System) (on MERN)",
  "private": true,
  "scripts": {
    "startHeroku": "node static/server.bundle.js",
    "start": "node server/www.js",
    "nm": "nodemon",
    "forever": "forever start --fifo --minUptime 1000 --spinSleepTime 1000 --killSignal=SIGINT static/server.bundle.js && forever list",
    "start2": "npm run compile && node static/www.js",
    "bundleServerDev": "webpack --config webpack.serverdev.js",
    "bundleServerWeb": "webpack --config webpack.serverdev.js",
    "build": "webpack",
    "buildnm": "webpack && npm run nm",
    "test": "npm run test:imports",
    "test:imports": "mocha \"./imports/**/*.spec.js\"",
    "compile": "node_modules/.bin/babel bin/www.js --out-dir static --source-map",
    "watch": "./node_modules/.bin/babel src -d static -watch"
  },
  "author": "asklim",
  "license": "ISC",
  "keywords": [
    "rsis",
    "heroku",
    "rsis web app"
  ],
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-loader": "^8.0.5",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "history": "^4.7.2",
    "http-errors": "~1.6.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.3.15",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.14.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-icons": "^3.5.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "request": "^2.88.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.2"
  },
  "//": "chai & mocha to '*' - last version",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "*",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prop-types": "^15.6.2",
    "react-bootstrap": "^0.32.4",
    "react-router": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "nodemonConfig": {
    "delay": "2000",
    "verbose": true,
    "execMap": {
      "js": "node --experimental-modules",
      "mjs": "node --experimental-modules"
    },
    "ext": "js,json,mjs",
    "watch": [
      "imports/**",
      "server/**",
      "api/**",
      "bin/*"
    ],
    "ignore": [
      ".git",
      ".vscode",
      "public/**",
      "node_modules/*",
      "node_modules/**/*",
      "node_modules/*/**",
      "node_modules/**",
      "src/**",
      "static/**"
    ]
  }
}
