{
  "name": "rsis-web",
  "version": "0.1.0",
  "description": "rsis (R Stock-In System) (on MERN)",
  "private": true,
  "homepage": "https://rsis-webapp.herokuapp.com",
  "directories": {
    "lib": "/imports"
  },
  "scripts": {
    "/1/": "*******   for Heroku    *******",
    "start": "node server/www.js",
    "build": "node scripts/build.js",
    "/2/": "*******  for development *******",
    "debug": "DEBUG=api:*,view:* node server/www.js",
    "debug:all": "DEBUG=* node server/www.js",
    "build:dev": "node scripts/devbuild.js",
    "start:srv": "node static/server.bundle.js",
    "start2": "npm run compile && node static/www.js",
    "compile": "node_modules/.bin/babel server/www.js --out-dir static --source-map",
    "bundleServerDev": "webpack --config webpack.serverdev.js",
    "bundleServerWeb": "webpack --config webpack.serverdev.js",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "forever": "forever start --fifo --minUptime 1000 --spinSleepTime 1000 --killSignal=SIGINT static/server.bundle.js && forever list",
    "jest:srv": "jest server/**/*.test.js",
    "jest:src": "jest ./src/**",
    "mocha:all": "npm run mocha:lib",
    "mocha:lib": "mocha \"./src/lib/**/*.spec.js\"",
    "mocha:rsis": "mocha \"./src/lib/rsis/*.spec.js\"",
    "mocha:work": "mocha \"./src/lib/rsis/work*.spec.js\""
  },
  "author": "asklim",
  "license": "ISC",
  "keywords": [
    "rsis",
    "heroku",
    "rsis web app"
  ],
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.1",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.5.1",
    "@types/googlemaps": "^3.38.0",
    "@types/markerclustererplus": "^2.1.33",
    "bfj": "^6.1.2",
    "body-parser": "^1.19.0",
    "browserslist": "^4.7.2",
    "caniuse-lite": "^1.0.30001008",
    "chartist": "^0.10.1",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "debug": "*",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fs-extra": "^7.0.1",
    "history": "^4.10.1",
    "http-errors": "^1.7.3",
    "isomorphic-fetch": "^2.2.1",
    "jest-codemods": "^0.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.8",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.4.0",
    "react": "^16.11.0",
    "react-chartist": "^0.13.3",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.11.0",
    "react-google-maps": "^9.4.5",
    "react-icons": "^3.8.0",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "react-swipeable-views": "^0.13.1",
    "request": "^2.88.0",
    "resolve": "^1.12.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.3"
  },
  "//": " '*' - last version",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "precss": "^4.0.0",
    "prop-types": "^15.6.2",
    "react-bootstrap": "^0.32.4",
    "react-router": "^5.1.0",
    "react-router-bootstrap": "^0.25.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "^3.7.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.0"
  },
  "nodemonConfig": {
    "delay": "2000",
    "verbose": true,
    "execMap": {
      "js": "node --experimental-modules",
      "mjs": "node --experimental-modules"
    },
    "ext": "js,json,mjs",
    "watch": [
      "imports/**",
      "server/**",
      "api/**",
      "bin/*"
    ],
    "ignore": [
      ".git",
      ".vscode",
      "public/**",
      "node_modules/*",
      "node_modules/**/*",
      "node_modules/*/**",
      "node_modules/**",
      "src/**",
      "static/**"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
