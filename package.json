  "//": " '*' - last version",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/eslint-parser": "^7.21.3",
    "@hot-loader/react-dom": "^17.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "jest": "^29.5.0",
