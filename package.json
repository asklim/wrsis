    "@svgr/webpack": "^6.1.2",
    "@types/googlemaps": "^3.38.0",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.14.2",
    "asklim": "git+https://asklim@github.com/asklim/asklim.git",
